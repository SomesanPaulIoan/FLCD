# Compute the sum of n numbers.

// main as function(0).

    n as integer.
    a as vector(integer).
    sum as integer.

    sum <- 0.
    n <- reader(input).
    // for i in (1, n).
        x as integer.
        x <- reader(input).
        sum <- sum + x.
    \\

    writer(output(1), sum).

    main <- 0.
\\